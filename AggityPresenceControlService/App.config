<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <!-- init log4net -->
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler,Log4net" />
  </configSections>
  <!-- end log4net -->
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
  </startup>
  <appSettings>
    <add key="DATABASE_FILE" value="c:\temp\AggityPresenceControlService.db" />
    <!-- <add key="WEBSERVICE_URL" value="http://localhost/AggityPresenceControlWS/api/punchdata" /> -->
    <!-- <add key="WEBSERVICE_URL" value="http://localhost:53010/api/punchdata" /> -->
    <add key="WEBSERVICE_URL" value="http://localhost/AggityPresenceControlWS_ASMX/PunchDataService.asmx" />

    <add key="TERMINAL_ID" value="1" />
    <add key="ClientSettingsProvider.ServiceUri" value="" />
  </appSettings>
  <!-- init log4net -->
  <log4net>
    <appender name="AggityPresenceControlAppender" type="log4net.Appender.RollingFileAppender">
      <file type="log4net.Util.PatternString" value="c:\temp\AggityPresenceControlService_%date{yyyy_MM_dd}.log" />
      <encoding value="utf-8" />
      <appendToFile value="true" />
      <rollingStyle value="Date" />
      <datePattern value="yyyy-MM-dd" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date %level [%thread] %type.%method - %message%n" />
      </layout>
    </appender>
    <root>
      <level value="All" />
      <!-- If the following line is not included the log file 
      will not be created even if log4net is configured with this file. -->
      <appender-ref ref="AggityPresenceControlAppender" />
    </root>
  </log4net>
  <!-- end log4net -->
</configuration>